@model Dinner;

<div id="dvMap" class="nerd-map" />
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD_ngoTftSnoCcKRmDhfJQ12v0RthRSw9o&callback=initMap"
        async defer></script>
<script>

    function initMap() {

        var lat = @(Model.Latitude) ==0 ?27.2038 :  @(Model.Latitude) ;
        var lng =  @(Model.Longitude) ==0? 77.5011 : @(Model.Longitude);

      

        var latlong = { lat, lng };
        var map = new google.maps.Map(document.getElementById('dvMap'), {
            zoom: 4,
            center: latlong
        });

        var image = 'http://chart.apis.google.com/chart?chst=d_map_pin_letter&chld=%E2%80%A2|FE7569';

        new google.maps.Marker({
            position: latlong,
            map: map,
            icon: image
        });


    }</script>